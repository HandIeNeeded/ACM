#include <bits/stdc++.h>

#define REP(i, a) for (int i = 0; i < (int) (a); i ++)
#define REPP(i, a, b) for (int i = (int) (a); i <= (int) (b); i ++)
#define MST(a, b) memset(a, b, sizeof(a))

using namespace std;

const int N = 1e5 + 5;
vector<int> edge[N];

int main() {
	ios :: sync_with_stdio(0);
	int n, m;
	cin >> n >> m;
	REPP(i, 1, m) {
		int x, y;
		cin >> x >> y;
		edge[x].push_back(y);
		edge[y].push(x);

	}

	return 0;
}

